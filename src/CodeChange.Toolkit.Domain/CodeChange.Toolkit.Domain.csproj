<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Product>Toolkit.NET</Product>
		<Description>A library containing domain tools, interfaces and base classes for Domain Driven Design (DDD) projects.</Description>
		<Copyright>Copyright © Code Change Ltd 2018</Copyright>
		<Authors>Code Change Ltd</Authors>
		<AssemblyVersion>1.0.0</AssemblyVersion>
		<Version>1.0.0</Version>
		<PackageLicenseUrl>https://github.com/codechangeltd/Toolkit.NET/blob/master/LICENSE</PackageLicenseUrl>
		<PackageProjectUrl>https://github.com/codechangeltd/Toolkit.NET</PackageProjectUrl>
		<RepositoryUrl>https://github.com/codechangeltd/Toolkit.NET</RepositoryUrl>
		<PackageIconUrl>https://raw.githubusercontent.com/codechangeltd/Toolkit.NET/master/src/CodeChange.Toolkit.Domain/Icon.png</PackageIconUrl>
		<RequireLicenseAcceptance>false</RequireLicenseAcceptance>
		<PackageReleaseNotes></PackageReleaseNotes>
		<Tags>CodeChange Toolkit Domain DDD .NET DOTNET C#</Tags>
	</PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\CodeChange.Toolkit\CodeChange.Toolkit.csproj" />
  </ItemGroup>

</Project>
